{
  "name": "@kaidohussar/ui",
  "version": "0.1.0",
  "description": "UI library by Kaido Hussar",
  "main": "dist/index.js",
  "scripts": {
    "analyze": "source-map-explorer 'lib/index.js'",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "start": "npm run storybook",
    "clean": "rimraf ./dist",
    "storybook": "start-storybook -p 9009 -s public",
    "stylelint": "stylelint --fix \"src/**/*.scss\"",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "typecheck": "tsc --noEmit",
    "release": "release-it",
    "check:codestyle:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "chromatic": "npx chromatic --project-token=461726485ea2",
    "build": "cross-env BABEL_ENV=production babel src -d dist"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "repository": "git+https://github.com/kaidohussar/ui.git",
  "keywords": [],
